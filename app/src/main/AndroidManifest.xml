<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tecsup.jeferson.serviceapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/service"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/service"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".PrincipalActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FirstPlantillaActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Administrator"/>
        <activity
            android:name=".SecondPlantillaActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Tecnico"/>
        <activity android:name=".ClienteActivity"
            android:theme="@style/AppTheme.NoActionBar"
            android:label="Cliente"></activity>
    </application>

</manifest>